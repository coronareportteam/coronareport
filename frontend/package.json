{
  "name": "quarano-frontend",
  "version": "0.0.0",
  "license": "EUPL-1.2-or-later",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "build-prod-evoila": "ng build --configuration=production-evoila",
    "build-sec-evoila": "ng build --configuration=sec-evoila",
    "build-stage": "ng build --configuration=stage",
    "build-test": "ng build --configuration=test",
    "build-sec": "ng build --configuration=sec",
    "build-autotest": "ng build --configuration=autotest",
    "prepare-deployment": "ncp ./deployment/pivotal/ ./dist/apps/quarano-frontend/",
    "prepare-deployment-evoila": "ncp ./deployment/evoila/ ./dist/apps/quarano-frontend/",
    "test": "ng test",
    "ci-test": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng test -- --silent --verbose",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint && npm run test"
    }
  },
  "private": true,
  "dependencies": {
    "@nrwl/angular": "9.3.0",
    "@angular/animations": "~9.1.3",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.1.3",
    "@angular/compiler": "~9.1.3",
    "@angular/core": "~9.1.3",
    "@angular/forms": "~9.1.3",
    "@angular/material": "^9.1.3",
    "@angular/material-moment-adapter": "^9.1.3",
    "@angular/platform-browser": "~9.1.3",
    "@angular/platform-browser-dynamic": "~9.1.3",
    "@angular/router": "~9.1.3",
    "@swimlane/ngx-datatable": "^17.0.0",
    "bootstrap-grid-only-css": "^4.1.3",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "nan": "^2.14.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.5",
    "subsink": "^1.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.1.3",
    "@angular/language-service": "~9.1.3",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/jest": "25.1.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^4.2.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "jest-zone-patch": "0.0.10",
    "ncp": "^2.0.0",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-jest": "25.2.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  }
}
