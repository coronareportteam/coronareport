{
  "name": "quarano-frontend",
  "version": "0.0.0",
  "license": "EUPL-1.2-or-later",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build-mannheim-prod": "ng build --configuration=production,mannheim-prod --base-href /mannheim/",
    "build-mannheim-stage": "ng build --configuration=production,mannheim-stage --base-href /mannheim-stage/",
    "build-cuxhaven-prod": "ng build --configuration=production,cuxhaven-prod --base-href /cuxhaven/",
    "build-cuxhaven-stage": "ng build --configuration=production,cuxhaven-stage --base-href /cuxhaven-stage/",
    "build-internal-dev-otc": "ng build --configuration=test,internal-dev-otc --base-href /internal-dev/",
    "build-internal-test-otc": "ng build --configuration=test,internal-test-otc --base-href /internal-test/",
    "build-demo-otc": "ng build --configuration=production,demo-otc --base-href /demo/",
    "prepare-deployment-otc": "ncp ./deployment/otc/ ./dist/apps/quarano-frontend/",
    "test": "ng test",
    "ci-test": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng test -- --silent --verbose",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "i18n:init": "ngx-translate-extract --input ./apps/quarano-frontend/src ./libs --output ./apps/quarano-frontend/src/assets/i18n/template.json --key-as-default-value --replace --format namespaced-json --key-as-default-value",
    "i18n:extract": "ngx-translate-extract --input ./apps/quarano-frontend/src ./libs --output ./apps/quarano-frontend/src/assets/i18n/template.json ./apps/quarano-frontend/src/assets/i18n/en.json ./apps/quarano-frontend/src/assets/i18n/de.json ./apps/quarano-frontend/src/assets/i18n/tr.json --sort --format namespaced-json --key-as-default-value"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(ts|scss|html)'"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.2",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~10.2.2",
    "@angular/compiler": "~10.2.2",
    "@angular/core": "~10.2.2",
    "@angular/forms": "~10.2.2",
    "@angular/material": "^9.1.3",
    "@angular/material-moment-adapter": "^9.1.3",
    "@angular/platform-browser": "~10.2.2",
    "@angular/platform-browser-dynamic": "~10.2.2",
    "@angular/router": "~10.2.2",
    "@ngrx/data": "^9.2.0",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "9.3.0",
    "@swimlane/ngx-datatable": "^17.0.0",
    "bootstrap-grid-only-css": "^4.1.3",
    "core-js": "^2.5.4",
    "country-flag-icons": "^1.2.3",
    "file-saver": "^2.0.2",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "nan": "^2.14.0",
    "rxjs": "~6.5.5",
    "subsink": "^1.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.2",
    "@angular/language-service": "~10.2.2",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@cypress/code-coverage": "^3.8.1",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/file-saver": "^2.0.1",
    "@types/jest": "25.1.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^4.2.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "jest-zone-patch": "0.0.10",
    "ncp": "^2.0.0",
    "ngx-build-plus": "^10.1.1",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "ts-jest": "25.2.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
